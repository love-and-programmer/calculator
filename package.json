{
  "name": "evol-calculator",
  "version": "1.0.0",
  "description": "This is a calculator project for the game -- Love and Producer",
  "author": "Yafufu",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate --spa",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "test": "jest",
    "format": "prettier --write '*/**/*.{js,vue}'",
    "prepareDev": "cp env/dev.env .env -f",
    "prepareTest": "cp env/test.env .env -f",
    "prepareProd": "cp env/prod.env .env -f",
    "generate:test": "npm-run-all prepareTest generate prepareDev",
    "generate:prod": "npm-run-all prepareProd generate prepareDev",
    "deploy": "gh-pages --dist --dotfiles=true"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.3.6",
    "ant-design-vue": "^1.4.10",
    "local-storage": "^2.0.0",
    "lodash": "^4.17.15",
    "node-sass": "^4.13.1",
    "nuxt": "^2.15.8",
    "nuxt-i18n": "^6.3.0",
    "sass-loader": "^8.0.0",
    "vue": "^2.6.11",
    "vue-i18n": "^8.15.0"
  },
  "devDependencies": {
    "@nuxtjs/dotenv": "^1.4.1",
    "@nuxtjs/eslint-config": "^1.0.1",
    "@nuxtjs/eslint-module": "^1.0.0",
    "@nuxtjs/moment": "^1.3.0",
    "@vue/test-utils": "^1.0.0-beta.27",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.0.1",
    "gh-pages": "^2.2.0",
    "jest": "^24.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.16.4",
    "vue-jest": "^4.0.0-0"
  }
}
